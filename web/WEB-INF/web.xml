<web-app>
    <display-name>Neighborhood Map Server</display-name>

    <servlet>
        <servlet-name>webservice</servlet-name>
        <servlet-class>org.mskcc.nms.servlet.WebService</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>webservice</servlet-name>
        <url-pattern>/retrieve-neighborhood-map.do</url-pattern>
    </servlet-mapping>
</web-app>
